{
	"manifest_version": 2,
	"name": "efmrl.link",
	"short_name": "efmrl",
	"description": "Quickly share or transfer ephemeral links between devices.",
	"version": "1.0.4",
	"browser_action": {
		"default_popup": "popup.html"
	},
	"permissions": ["activeTab"],
	"icons": {
		"16": "images/efmrl-16.png",
		"48": "images/efmrl-48.png",
		"128": "images/efmrl-128.png",
		"256": "images/efmrl-256.png"
	}
}
